{
  "spinnerStream": "stdout",
  "progressStream": "stdout",
  "reasonerStream": "stderr",
  "options": [
    {
      "mode": "create",
      "project": "./libs/canvas/tsconfig.json",
      "exportFilename": "index.ts",
      "useSemicolon": true,
      "useBanner": true,
      "useTimestamp": true,
      "quote": "'",
      "directive": "",
      "fileExt": "none",
      "overwrite": true,
      "backup": false,
      "generationStyle": "auto",
      "include": [
        "src/**/*.ts"
      ],
      "exclude": [
        "../../node_modules",
        "../../tmp"
      ],
      "skipEmptyDir": true,
      "startFrom": "./libs/canvas",
      "output": "./libs/canvas/src",
      "removeBackup": true,
      "forceYes": true
    }
  ]
}
